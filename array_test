window.onload = function () {

    var array1 = [34, "hello", true, "bye", "password", "35469gff", 423];

    var position = parseInt (prompt("posicion del array: "));

    for(var i = 0; i < array1.length();i++){

        if (i==position){
            var resultado = array1[i];
        }
        alert(resultado);
    }
}
    </script>
